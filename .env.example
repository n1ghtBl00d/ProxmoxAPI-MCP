# .env.example - Copy this to .env and fill in your Proxmox details
PROXMOX_HOST=your_proxmox_ip_or_hostname
PROXMOX_USER=your_user@pam  # Or your_user@pve if using PVE realm
PROXMOX_PASSWORD=your_password
# Optional: Set to 'true' or '1' to disable SSL certificate verification (use with caution)
PROXMOX_VERIFY_SSL=false
